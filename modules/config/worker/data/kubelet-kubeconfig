apiVersion: v1
kind: Config
users:
- name: kubelet-bootstrap
  user:
    token: ${token}
clusters:
- name: local
  cluster:
    certificate-authority: /etc/kubernetes/ca.crt
    server: https://${master_ip}
contexts:
- context:
    cluster: local
    user: kubelet-bootstrap
  name: service-account-context
current-context: service-account-context
